@page
@using EasyAbp.EzGet.Admin.Web.Pages
@using EasyAbp.EzGet.Admin.Web.Pages.EzGet.Credentials
@using EasyAbp.EzGet.Localization
@using Microsoft.Extensions.Localization

@model SelectCredentialModalModel

@inject IStringLocalizer<EzGetResource> L

@{
    Layout = null;
}

<form data-check-form-on-close="false">
    <abp-modal size="Large">
        <abp-modal-header title="@L["SelectCredential"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-table striped-rows="true" class="nowrap credentials-table"></abp-table>
        </abp-modal-body>
    </abp-modal>
</form>